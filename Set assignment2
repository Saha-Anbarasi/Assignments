import java.util.HashSet;

public class set2 {
	public static void main(String[] args)
	{
		
		HashSet <String> set = new HashSet<String>();
		
		set.add("Square");
		set.add("Circle");
		set.add("Tringangle");
		set.add("Circle");
		set.add("Rhombus");
		set.add("pentagon");
		set.add("line");
		set.add("word");
		
		
		System.out.println("HashSet : " + set);
		
		
		set.remove("word");
		set.remove("line");
		
		System.out.println("Modified HashSet : " + set);
	}
}
