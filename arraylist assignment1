import java.util.*;


public class arraylist1{
	
	public static void main (String[] args)
	{
		List<String> list = new ArrayList<String>();
		
		list.add("Square");
		list.add("Triangle");
		list.add("Circle");
		list.add("Rhombus");
		
		int size = list.size();
		
		
		System.out.println("before assignment = " + list);
		System.out.println("Size of list = " + size);
		
	}
}
